    <div class="container">
    <table class="responsive-table striped center-align">
        <thead>
          <tr>
              <th> Venue </th>
              <th> Date </th>
              <th> Price </th>
          </tr>
        </thead>

        <tbody>
            {{#each event}}
              <tr> 
                <td> {{this.Venue}} </td>
                <td> {{this.Date}} </td>
                <td> {{this.Price}} </td>
              </tr>
            {{/each}}
        </tbody>
      </table>
