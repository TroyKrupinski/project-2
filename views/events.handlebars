    <form method="GET" action="/eventsQuery">
            <p> Band Name </p>
            <input type="text" name="bandName" style="width: 50%" placeholder="Type Band Name Here" value>
            
            <p> Venue</p>
            <input type="text" name="venue" style="width: 50%" placeholder="Type Venue Name Here">

            <button href="/eventsQuery" type="submit"> Submit </button>
        </form>


